# Задание 1

a <- 4
b <- 7.7
A <- "Hello world"

round(a / b, 2)
a < b
paste(A, A, A, sep = ",")

# Задание 2 часть 1
for (x in 2021:2023) {
    print(paste("Маркетинговый проект запустился в", x, "году"))
  x <- x + 1
}

# Задание 2 часть 2
for (x in 2021:2030) {
  if (x <= 2023) {
    print(paste("Маркетинговый проект запустился в", x, "году"))
}  else {
    print(paste("Маркетинговый проект будет запущен в", x, "году"))
  }
  x <- x + 1
}
